{
  "name": "search-neu",
  "version": "1.0.0",
  "scripts": {
    "start": "cross-env NODE_ENV=dev babel-node backend/server.js",
    "scrape": "babel-node backend/scrapers/main.js",
    "build": "mkdir -p public; cp frontend/robots.txt public/robots.txt; cp frontend/sw.js public/sw.js; cross-env NODE_ENV=prod ./node_modules/webpack/bin/webpack.js --config ./backend/webpack.config.babel.js --profile --colors",
    "lint": "./node_modules/eslint/bin/eslint.js backend/scrapers",
    "test": "./node_modules/jest-cli/bin/jest.js",
    "deploy": "gh-pages -d public",
    "download_data": "time git clone -b gh-pages --single-branch git@github.com:ryanhugh/searchneu.git public; rm -rf public/.git"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-1"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-object-rest-spread",
      "react-hot-loader/babel"
    ]
  },
  "_comment": "Do not upgrade gh-pages to 1.x until travis's version of git is >=1.9. Right now it is 1.8 and the latest is like 2.10.",
  "_comment2": "domutils upgrade is faster for parsing html (stack instead of recursive) but requires some code changes. Shoudn't be too bad.",
  "dependencies": {
    "@mapbox/togeojson": "^0.16.0",
    "acorn": "^5.0.3",
    "another-name-parser": "^0.0.4",
    "async": "^2.1.5",
    "cheerio": "^0.22.0",
    "classnames": "^2.2.5",
    "cookie": "^0.3.1",
    "cross-env": "^5.0.0",
    "deep-diff": "^0.3.8",
    "dns-then": "^0.1.0",
    "domutils": "1.5.1",
    "elasticlunr": "^0.9.5",
    "express": "^4.15.2",
    "express-async-wrap": "^1.0.0",
    "file-loader": "^0.11.1",
    "fs-promise": "^2.0.0",
    "gh-pages": "^0",
    "he": "^1.1.1",
    "html-webpack-plugin": "^2.28.0",
    "htmlparser2": "^3.9.2",
    "identity-obj-proxy": "^3.0.0",
    "is-mobile": "^0.2.2",
    "jest": "^20.0.1",
    "lodash": "^4.17.4",
    "mkdirp-promise": "^5.0.1",
    "mockdate": "^2.0.1",
    "moment": "^2.17.1",
    "object-hash": "^1.1.7",
    "prop-types": "^15.5.8",
    "react": "^15.4.2",
    "react-bootstrap": "^0.31.0",
    "react-css-modules": "^4.1.0",
    "react-dom": "^15.4.2",
    "react-tooltip": "^3.2.6",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.3",
    "resolve-url-loader": "^2.0.2",
    "semantic-ui-css": "^2.2.4",
    "urijs": "^1.18.8",
    "whois": "^2.3.9",
    "xmldom": "^0.1.27"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-runtime": "^6.20.0",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "ignore-styles": "^5.0.1",
    "react-hot-loader": "^3.0.0-beta.2",
    "style-loader": "^0.17.0",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.16.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.svg$": "<rootDir>/frontend/components/tests/mockSvg.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
